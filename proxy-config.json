{
    "/countries": {
        "target": "https://api.first.org/data/v1", 
        "secure": true, 
        "logLevel": "debug",
        "changeOrigin": true
    }, 
    "/api": {
        "target": "https://api.openweathermap.org/data/2.5", 
        "secure": true, 
        "logLevel": "debug",
        "pathRewrite": {
            "^/api/forecast": "/forecast", 
            "^/api/weather": "/weather"
        }, 
        "changeOrigin": true
    }
}